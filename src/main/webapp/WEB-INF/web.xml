    <servlet>
        <servlet-name>AddPhoneServlet</servlet-name>
        <servlet-class>com.example.practicalmodel.controller.AddPhoneServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddPhoneServlet</servlet-name>
        <url-pattern>/admin/phones/create</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PhoneListServlet</servlet-name>
        <servlet-class>com.example.practicalmodel.controller.PhoneListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PhoneListServlet</servlet-name>
        <url-pattern>/admin/phones/list</url-pattern>
    </servlet-mapping>
